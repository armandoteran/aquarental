<%= simple_form_for (@equipment) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :category %>
  <%= f.input :location %>
  <%= f.input :picture_url %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :price_day %>
  <%= f.submit %>
<% end %>

