<div class="container">
  <div class="row">
    <div class="-col-6 m-auto">

      <%= form_for [@equipment, @booking] do |f| %>
        <%= f.label :start %>
        <%= f.date_field :start_date %>
        <%= f.label :end %>
        <%= f.date_field :end_date %>
        <%= f.submit %>
      <% end %>

    </div>
  </div>
</div>
